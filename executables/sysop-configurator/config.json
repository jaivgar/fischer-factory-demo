{
  "interfaces": [
    "HTTP-SECURE-HTML",
    "HTTP-SECURE-JSON"
  ],
  "serviceRegistryHost": "127.0.0.1:8443",
  "providers": [
    {
      "address": "127.0.0.1",
      "authenticationInfo": "",
      "port": 9001,
      "systemName": "buyer"
    },
    {
      "address": "127.0.0.1",
      "authenticationInfo": "",
      "port": 9002,
      "systemName": "seller"
    },
    {
      "address": "127.0.0.1",
      "authenticationInfo": "",
      "port": 9003,
      "systemName": "middleware"
    }
  ],
  "services": [
    {
      "serviceDefinition": "event-subscriber",
      "providerIndex": 0,
      "serviceUri": "/events/buyer",
      "secure": "CERTIFICATE",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    },
    {
      "serviceDefinition": "buyer",
      "providerIndex": 0,
      "serviceUri": "/buyer",
      "secure": "TOKEN",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    },
    {
      "serviceDefinition": "event-subscriber",
      "providerIndex": 1,
      "serviceUri": "/events/seller",
      "secure": "CERTIFICATE",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    },
    {
      "serviceDefinition": "middleware",
      "providerIndex": 2,
      "serviceUri": "/middleware",
      "secure": "TOKEN",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    },
    {
      "serviceDefinition": "event-subscriber",
      "providerIndex": 2,
      "serviceUri": "/events/middleware",
      "secure": "CERTIFICATE",
      "interfaces": [
        "HTTP-SECURE-JSON"
      ]
    }
  ],
  "rules": [
    {
      "consumer": "buyer",
      "services": [
        "event-subscribe",
        "event-unsubscribe"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "buyer",
      "services": [
        "trusted-contract-negotiation",
        "trusted-contract-observation"
      ],
      "providers": [
        "contract_proxy_buyer"
      ]
    },
    {
      "consumer": "contract_proxy_buyer",
      "services": [
        "contract-negotiation"
      ],
      "providers": [
        "contract_proxy_seller"
      ]
    },
    {
      "consumer": "contract_proxy_buyer",
      "services": [
        "event-publish"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "contract_proxy_seller",
      "services": [
        "contract-negotiation"
      ],
      "providers": [
        "contract_proxy_buyer"
      ]
    },
    {
      "consumer": "contract_proxy_seller",
      "services": [
        "event-publish"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "seller",
      "services": [
        "event-subscribe",
        "event-unsubscribe"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "seller",
      "services": [
        "trusted-contract-negotiation",
        "trusted-contract-observation"
      ],
      "providers": [
        "contract_proxy_seller"
      ]
    },
    {
      "consumer": "middleware",
      "services": [
        "event-subscribe",
        "event-unsubscribe"
      ],
      "providers": [
        "event_handler"
      ]
    },
    {
      "consumer": "middleware",
      "services": [
        "trusted-contract-observation"
      ],
      "providers": [
        "contract_proxy_seller"
      ]
    },
    {
      "consumer": "middleware",
      "services": [
        "buyer"
      ],
      "providers": [
        "buyer"
      ]
    },
    {
      "consumer": "smart_product",
      "services": [
        "middleware"
      ],
      "providers": [
        "middleware"
      ]
    },
    {
      "consumer": "smart_product",
      "services": [
        "wmanager-workstation-operations"
      ],
      "providers": [
        "workflow_manager"
      ]
    },
    {
      "consumer": "workflow_manager",
      "services": [
        "execute-workflow",
	"provide-workflows-in-execution",
	"provide-workflows-type"
      ],
      "providers": [
        "workflow_executor"
      ]
    },
    {
      "consumer": "workflow_executor",
      "services": [
        "wmanager-operation-results"
      ],
      "providers": [
        "workflow_manager"
      ]
    },
    {
      "consumer": "workflow_executor",
      "services": [
        "sensorvalue",
	"actuatorvalue"
      ],
      "providers": [
        "opc_ua_provider"
      ]
    },
    {
      "consumer": "workflow_executor",
      "services": [
        "execute-workflow",
	"provide-workflows-in-execution",
	"provide-workflows-type"
      ],
      "providers": [
        "workflow_executor"
      ]
    }
  ]
}
